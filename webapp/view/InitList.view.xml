<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="team12.Team12_Fish.controller.InitList"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Fish Listing">
				<subHeader>
					<Toolbar>
						<SearchField id="idSearchField"
							search="onSearch"
							width="100%" />
					</Toolbar>
				</subHeader>
				<content>
					<Table
						id="idList"
						items="{local>/results}" >
						<columns>
							<Column>
							</Column>
							<Column>
								<Text text="Name" />
							</Column>
							<Column>
								<Text text="Average Price" />
							</Column>
							<Column>
								<Text text="Price Range" />
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Image
										src="{img>/products/pic1}"
										densityAware="false" >
									</Image>
									<ObjectIdentifier
										title="{local>name}"/>
									<ObjectNumber
										number="RM {local>average_price}"
										unit="/{local>uom}" />
									<ObjectNumber
										number="RM{local>lower_range} - RM{local>higher_range}"
										unit="/{local>uom}" />
								</cells>
							</ColumnListItem>
						</items>
					</Table>
					
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>